<div>
    <h1>List of products:</h1>
    
    <ul>
        {{#each products}}
            <li>
                <h3>{{title}}</h3>
                <p>{{description}}</p>
                <p>Code: {{code}}</p>
                <p>Price: ${{price}}</p>
                <p>Status: {{status}}</p>
                <p>Stock: {{stock}}</p>
                <p>Category: {{category}}</p>
                <p>Thumbnail: {{thumbnail}}</p>
                <p>Id: {{id}}</p>
            </li>
        {{/each}}
    </ul>
</div>